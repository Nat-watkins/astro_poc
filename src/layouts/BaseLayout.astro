---
import Head from "../components/Head.astro";
import Banner from "../components/Banner.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import Identifier from "../components/Identifier.astro";
import InPageNav from "components/InPageNav.astro";
import "#assets/sass/styles.scss";
const {pageTitle} = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<meta charset="utf-8" />
	<Head />
	<body id="page-top">
		<script src="#assets/uswds/js/uswds.js"></script>
        <a class="usa-skipnav z-top" href="#main-content">Skip to main content</a>
		<Banner />
        <Header />
            <div class="usa-in-page-nav-container">
                <main id="main-content" class="main-content position-relative grid-container padding-y-4">
                    <h1>{pageTitle}</h1>
                    <slot />
                </main>
            </div>
		<Footer />
        <Identifier />
	</body>
</html>
